/*             */
/*  GLOBAL UI  */
/*             */
/*#Window{
  background: rgb(30,30,30);
}*/
#Window_TabWidget::pane{
  background: transparent;
  border:0;
}
#Window_TabWidget_TabBar::tab{
  background: transparent;
  color: white;
  height: 23px;
}
#Window_TabWidget_TabBar::tab:hover{
  background-color: rgba(255,255,255,1);
  color: black;
}
#Window_TabWidget_TabBar::tab:selected{
  background-color:#363535;
  color: #008BEA;
}

.QLabel{
  border: 1px solid rgba(200,20,20,1);
  color: white;
}

.QPushButton{
  background: transparent;
  border: 1px solid rgba(20,200,200,1);
  color: white;
}

#Quit_Button{
  background: transparent;
  border-radius: 20px; 
}
#Quit_Label{
  border: none;
}
#Background{
  border: none;

}

QLineEdit{
  background: transparent;
  border: 1px solid white;
  color: white;
}

/*             */
/*  PLAYER UI  */
/*             */
/*QListWidget*/
QListWidget{
  background:rgba(50,50,50,0.3);
  border:0;
  color:white;
  border-radius:2;

}
#PlayerSongList::Item:Hover{ 
  background:rgba(50,200,50,0);
}
#PlayerSongList::Item::Selected{
  background: rgba(100,100,255,0);
  border: none;
  /*border: 1px solid white;*/
  color: rgb(0, 200, 200);

}
#PlayerSongList::Item::Selected:Hover{

}
#PlayerSongList_VerticalScrollBar:vertical{ /*QListWidget::Scrollbar*/
  background-color:#2A2929;
  width:10px;
  margin:15px3px15px3px;
  border:1px transparent #2A2929;
  border-radius:4px;
}
#PlayerSongList_VerticalScrollBar::handle:vertical{
  background-color:#008BEA;
  /*#605F5F;
  */
  min-height:5px;
  border-radius:4px;
}
#PlayerSongList_VerticalScrollBar::sub-line:vertical{
  margin:3px0px3px0px;
  background:transparent;
  height:10px;
  width:10px;
  subcontrol-position:top;
  subcontrol-origin:margin;
}
#PlayerSongList_VerticalScrollBar::add-line:vertical{
  margin:3px0px3px0px;
  background:transparent;
  height:10px;
  width:10px;
  subcontrol-position:bottom;
  subcontrol-origin:margin;
}
#PlayerSongList_VerticalScrollBar::sub-line:vertical:hover,#PlayerSongList_VerticalScrollBar::sub-line:vertical:on{
  background:transparent;
  height:10px;
  width:10px;
  subcontrol-position:top;
  subcontrol-origin:margin;
}
#PlayerSongList_VerticalScrollBar::add-line:vertical:hover,#PlayerSongList_VerticalScrollBar::add-line:vertical:on{
  background:transparent;
  height:10px;
  width:10px;
  subcontrol-position:bottom;
  subcontrol-origin:margin;
}
#PlayerSongList_VerticalScrollBar::up-arrow:vertical,#PlayerSongList_VerticalScrollBar::down-arrow:vertical{
  background:none;
}
#PlayerSongList_VerticalScrollBar::add-page:vertical,#PlayerSongList_VerticalScrollBar::sub-page:vertical{
  background:none;
}

QLCDNumber{
  color: white;
  background: transparent;
  border: 1px solid white;
}

QSlider::groove:horizontal{
  border-top:  11px solid transparent;
  border-bottom: 11px solid transparent;
}
QSlider::groove:horizontal:hover{
  border: 0px solid white;
}
QSlider::add-page:horizontal{
  background: transparent;
  margin-top: 6px;
  margin-bottom: 6px;
  border-radius: 4px;
}
QSlider::sub-page:horizontal{
  background: grey;
  margin-top: 8px;
  margin-bottom: 8px;
  border-radius: 3px;
}
#VolumeSlider::handle:horizontal{
  background: #008BEA;
  border-left: 3px solid #008BEA;
  border-right:3px solid #008BEA;
  height: 1px;
  margin-right: 0px;
}
#MusicTimeSlider::handle:horizontal{
  background: transparent;
  border-left: 3px solid transparent;
  border-right:3px solid transparent;
  height: 1px;
  margin-right: -4px;
}
QSlider::handle:horizontal:hover{
  width: 5px;
  border-radius: 4px;
}
QSlider::handle:horizontal:hold{
  height: 5px;
  width: 5px;
  border-radius: 4px;
} 

#VolumeSlideStyleLabel{
  background: rgb(50,50,50);
  margin-top:7px;
  margin-bottom:7px;
  border-radius: 4px;
  border: none;
}

