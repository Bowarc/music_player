#Downloader_DLButton{
  border-width: 2px;
  border-style: solid;
  border-color: qlineargradient( /*transparent*/
      x0:1 y0:1,
      x1:0 y1:1,
      stop:1 rgba(255,255,255,1),
      stop:0.55 rgba(180,180,180,0.0), 
      stop:0.45 rgba(180,180,180,0.0), 
      stop:0 rgba(255,255,255,1));
  color : white;
  background: qlineargradient(
      x0:1 y0:1,
      x1:0 y1:1,
      stop:1 rgba(0,139,234,1),
      stop:0.75 rgba(180,180,180,0.0), 
      stop:0.25 rgba(180,180,180,0.0), 
      stop:0 rgba(0,139,234,1));
  font-size: 20px ;
}

